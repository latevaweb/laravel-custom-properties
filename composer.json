{
  "name": "latevaweb/laravel-custom-properties",
  "description": "Laravel trait to make add dynamic properties to Eloquent models",
  "keywords": [
    "properties",
    "dynamic",
    "attributes",
    "eloquent",
    "laravel"
  ],
  "homepage": "https://github.com/latevaweb/laravel-custom-properties",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Marc Guinea",
      "email": "marc@latevaweb.com",
      "homepage": "https://www.latevaweb.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^7.3",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "orchestra/testbench": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "LaTevaWeb\\CustomProperties\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaTevaWeb\\CustomProperties\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  }
}
